<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Disabled -->
    <item android:alpha="0.3" android:color="@color/text_field_stroke" android:state_enabled="false" />

    <!-- Focused -->
    <item android:alpha="0.3" android:color="@color/text_field_stroke" android:state_focused="true" />

    <!-- Hovered -->
    <item android:alpha="0.3" android:color="@color/text_field_stroke" android:state_hovered="true" />

    <!-- Normal -->
    <item android:alpha="0.3" android:color="@color/text_field_stroke" />
</selector>
